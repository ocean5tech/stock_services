{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel/stock-analysis.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/vercel/stock-analysis",
      "dest": "/api/vercel/stock-analysis.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}